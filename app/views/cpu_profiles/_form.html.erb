<% semantic_form_for @cpu_profile do |f| %>
  <%= f.error_messages %>
  <% f.inputs do %>
    <%= f.input :api_name, :hint => 'API name of the CPU Profile, for example, i386' %>
    <%= f.input :name, :hint => 'Name the Profile, for example, 32-bit platform' %>
    <%= f.input :desc, :hint => 'Description, for example, 10 Gigabit Ethernet',
      :required => false %>
  <% end %>
  <% f.buttons do %>
    <%= f.submit %>
    <%= submit_tag 'Cancel', :name => 'cancel_button' %>
  <% end %>
<% end %>
