<h1>Listing instance_type_categories</h1>

<table>
  <tr>
    <th>Provider</th>
    <th>Name</th>
    <th>Description</th>
    <th>Position</th>
  </tr>

<% for instance_type_category in @instance_type_categories %>
  <tr>
    <td><%=h instance_type_category.provider_id %></td>
    <td><%=h instance_type_category.name %></td>
    <td><%=h instance_type_category.description %></td>
    <td><%=h instance_type_category.position %></td>
    <td><%= link_to 'Show', instance_type_category %></td>
    <td><%= link_to 'Edit', edit_instance_type_category_path(instance_type_category) %></td>
    <td><%= link_to 'Destroy', instance_type_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New instance_type_category', new_instance_type_category_path %>
