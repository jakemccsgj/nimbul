<h1>Listing instance_kind_categories</h1>

<table>
  <tr>
    <th>Provider</th>
    <th>Name</th>
    <th>Description</th>
    <th>Position</th>
  </tr>

<% for instance_kind_category in @instance_kind_categories %>
  <tr>
    <td><%=h instance_kind_category.provider_id %></td>
    <td><%=h instance_kind_category.name %></td>
    <td><%=h instance_kind_category.description %></td>
    <td><%=h instance_kind_category.position %></td>
    <td><%= link_to 'Show', instance_kind_category %></td>
    <td><%= link_to 'Edit', edit_instance_kind_category_path(instance_kind_category) %></td>
    <td><%= link_to 'Destroy', instance_kind_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New instance_kind_category', new_instance_kind_category_path %>
