<% subtitle h(@provider_account.nil? ? '' : @provider_account.name + ': ') + 'Security Groups' %>

<h1><%= pagetitle h(@provider_account.nil? ? '' : @provider_account.name + ': ') + 'Security Groups' %></h1>

<br />

<% form_for :provider_account,
    :url => provider_account_path(@provider_account),
    :html => { :method => :post } do |f| %>
  <%= render :partial => "security_groups/list", :object => @provider_account, :locals => { :f => f }  %>
<% end %>
