<%= javascript_tag "Editable.setupAll();" %>
<div id="daemons">
<table>
    <tr>
        <th><%= select_all_check_box 'select_all' %></th>
        <th>Name</th>
        <th>State</th>
        <th>PID</th>
        <th>Last Log Entry</th>
        <th>Runtime</th>
    </tr>
    <tbody id="daemon_records">
        <%= render :partial => "daemon", :collection => @daemons %>
    </tbody>
</table>
</div>
