<h1>Listing account_groups</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% for account_group in @account_groups %>
  <tr>
    <td><%=h account_group.name %></td>
    <td><%=h account_group.description %></td>
    <td><%= link_to 'Show', account_group %></td>
    <td><%= link_to 'Edit', edit_account_group_path(account_group) %></td>
    <td><%= link_to 'Destroy', account_group, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New account_group', new_account_group_path %>
