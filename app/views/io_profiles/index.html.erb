<h1>Listing io_profiles</h1>

<table>
  <tr>
    <th>Position</th>
    <th>Name</th>
    <th>Desc</th>
  </tr>

<% for io_profile in @io_profiles %>
  <tr>
    <td><%=h io_profile.position %></td>
    <td><%=h io_profile.name %></td>
    <td><%=h io_profile.desc %></td>
    <td><%= link_to 'Show', io_profile %></td>
    <td><%= link_to 'Edit', edit_io_profile_path(io_profile) %></td>
    <td><%= link_to 'Destroy', io_profile, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New io_profile', new_io_profile_path %>
