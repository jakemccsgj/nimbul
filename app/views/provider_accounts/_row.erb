<td class="select-handle"><%= better_check_box_tag "provider_account_ids[]", provider_account.id, false, :provider_account %></td>
<td>
  <%= link_to h(provider_account.name), provider_account %>
  <%= parent_stats_link('Cost Details', provider_account) %>
</td>
<td><%= h(provider_account.account_group.nil? ? 'Not Set' : provider_account.account_group.name) %></td>
<td><%= h(provider_account.description) %></td>
<td><%= provider_account.instances.count %></td>
<td id="provider_account_<%= provider_account.id %>_total">&nbsp;</td>
<td id="provider_account_<%= provider_account.id %>_projection">&nbsp;</td>
<td>
  <%= time_ago(provider_account.refreshed_at) %>
  <%= parent_stats_js(provider_account) %>
</td>
